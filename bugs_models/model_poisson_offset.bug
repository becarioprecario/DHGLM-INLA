model {

  for(i in 1:n) {
    y[i] ~ dpois(mu[i])
    log(mu[i]) <- log(offset[i]) + beta[1] + beta[2] * x_mean1[i] + reff[i] 
    reff[i] ~ dnorm(0, prec[i])
    log(prec[i]) <- gamma[1] + gamma[2] * x_prec1[i]
  }

  beta[1] ~ dnorm(0, 0.001)
  beta[2] ~ dnorm(0, 0.001)
  gamma[1] ~ dnorm(0, 0.001)
  gamma[2] ~ dnorm(0, 0.001)

}
